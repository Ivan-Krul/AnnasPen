  <ItemGroup>
    <PackageReference Include="Raylib-cs" Version="4.2.0.2" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\AnnasUI\AnnasUI.csproj" />
  </ItemGroup>

</Project>
